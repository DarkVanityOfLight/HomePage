smokeyBlack = rgba(12, 15, 10, 1)
white = rgba(239, 249, 240, 1)
skyBlue = rgba(146, 220, 229, 1)
marineBlue = rgba(21, 50, 67, 1)
grey = rgba(49, 62, 80, 1)


.start-container
    width: 100%
    height: fit-content
    display: flex
    flex-direction: row
    flex-wrap: wrap

.quote-super-container
    height fit-content
    width 100%
    min-width: 100%

.skill-outer-super-container
    height: fit-content
    width: 100%

.project-outer-super-container
    height fit-content
    width: 100%
    margin-top: 30px

.skill-header
    width fit-content
    height fit-content
    font-size 35px
    color #2E312E
    margin 20px auto

.skill-header p
    display inline

.skill-header:hover
    color #616163 
    font-size 40px
    cursor pointer

.skill-super-container
    display flex
    justify-content center
    flex-wrap: wrap

.project-super-container
    display: flex
    flex-wrap: wrap
    flex-direction: column
    align-items: stretch